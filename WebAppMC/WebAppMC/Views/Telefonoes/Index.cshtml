            @Html.DisplayFor(modelItem => item.Updated_At)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Entidad.Nombre)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Codigo_Id }) |
